<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/menu_recordings_default" android:tint="?attr/drawableTintOverColor"/>
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/menu_recordings_default" android:tint="?attr/drawableTintDisabledColor"/>
    </item>
    <item>
        <bitmap android:src="@drawable/menu_recordings_default" android:tint="?attr/drawableTintColor"/>
    </item>
</selector>
